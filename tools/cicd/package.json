    "ts-node": "^10.0.0",
    "typescript": "~4.4.4"
  },
  "dependencies": {
    "@aws-cdk/assert": "^1.129.0",
    "@aws-cdk/aws-codebuild": "^1.129.0",
    "@aws-cdk/aws-codepipeline": "^1.129.0",
    "@aws-cdk/aws-codepipeline-actions": "^1.129.0",
    "@aws-cdk/aws-iam": "^1.129.0",
    "@aws-cdk/core": "^1.129.0",
    "source-map-support": "^0.5.19"
