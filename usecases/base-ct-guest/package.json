{
  "private": true,
  "name": "blea-base-ct-guest",
  "version": "1.0.0",
  "description": "Baseline for AWS Control Tower guest accounts",
  "license": "MIT-0",
  "bin": {
    "blea-base-ct-guest": "bin/blea-base-ct-guest.js"
  },
  "scripts": {
    "synth:dev": "npx cdk synth -c environment=dev",
    "depcheck": "npx depcheck --ignore-dirs cdk.out",
    "build": "tsc --build",
    "clean": "tsc --build --clean && rm -rf cdk.out",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint --fix .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "aws-cdk": "^2.39.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.5.1",
    "prettier": "^2.7.1",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.4"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.38.1",
    "cdk": "^2.38.1",
    "constructs": "^10.1.91"
  }
}
