{
  "private": true,
  "name": "blea-base-ct-audit",
  "version": "1.0.0",
  "description": "Baseline for AWS Control Tower audit account",
  "license": "MIT-0",
  "bin": {
    "blea-base-ct-audit": "bin/blea-base-ct-audit.js"
  },
  "scripts": {
    "synth:dev": "npx cdk synth -c environment=dev-audit",
    "depcheck": "npx depcheck --ignore-dirs cdk.out",
    "build": "tsc --build",
    "clean": "tsc --build --clean && rm -rf cdk.out",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint --fix .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "18.11.10",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.45.0",
    "aws-cdk": "^2.52.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.5.1",
    "prettier": "^2.8.0",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.49.0",
    "cdk": "^2.54.0",
    "constructs": "^10.1.186"
  }
}
