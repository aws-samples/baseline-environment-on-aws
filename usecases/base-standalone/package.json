    "aws-cdk-lib": "^2.59.0",
    "constructs": "^10.1.213",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.5.1",
    "prettier": "^2.8.2",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.59.0",
    "cdk": "^2.58.1",
    "constructs": "^10.1.213"
  }
