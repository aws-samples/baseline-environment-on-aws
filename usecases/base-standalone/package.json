{
  "private": true,
  "name": "blea-base-standalone",
  "version": "1.0.0",
  "description": "Baseline for standalone accounts",
  "license": "MIT-0",
  "bin": {
    "blea-base-sa": "bin/blea-base-sa.js"
  },
  "scripts": {
